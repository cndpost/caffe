
name: "train_val"

layer {
  name: "data"
  type: "ImageData"
  top: "data"
  top: "label"
  include {
    phase: TRAIN
  }
  transform_param {
    mirror: true
    mean_file: "models/celeb_siamese_v1/celeb34372_mean.binaryproto"
  }
  image_data_param {
    source: "models/celeb_siamese_v1/clusters_2_seed=0_trn_siamese.txt"
    batch_size: 25
  }
}
layer {
  name: "data"
  type: "ImageData"
